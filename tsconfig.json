{
  "compileOnSave": false,
  "compilerOptions": {
    "baseUrl": "./",
    "outDir": "./dist/out-tsc",
    "sourceMap": true,
    "declaration": false,
    "downlevelIteration": true,
    "experimentalDecorators": true,
    "module": "es2020",
    "moduleResolution": "node",
    "importHelpers": true,
    "target": "ES2022",
    "typeRoots": [
      "node_modules/@types"
    ],
    "lib": [
      "es2018",
      "dom"
    ],
    "paths": {
      "ngx-translate-extract-marker": [
        "dist/ngx-translate-extract-marker"
      ],
      "ngx-translate-extract-marker/*": [
        "dist/ngx-translate-extract-marker/*"
      ]
    },
    "useDefineForClassFields": false
  }
}